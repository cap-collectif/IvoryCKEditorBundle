{
  "name": "egeloen/ckeditor-bundle",
  "type": "symfony-bundle",
  "description": "Provides a CKEditor integration for your Symfony2 Project.",
  "keywords": [
    "ckeditor"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Eric GELOEN",
      "email": "geloen.eric@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/pgodel/ivory-json-builder"
    }
  ],
  "require": {
    "php": "^7.0",
    "egeloen/json-builder": "dev-feature/symfony4",
    "symfony/dependency-injection": "^3.3|^4.0",
    "symfony/form": "^3.3|^4.0",
    "symfony/framework-bundle": "^3.3|^4.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.0",
    "phpunit/phpunit": "^6.0",
    "symfony/asset": "^3.3|^4.0",
    "symfony/console": "^3.3|^4.0",
    "symfony/phpunit-bridge": "^3.3|^4.0",
    "symfony/templating": "^3.3|^4.0",
    "symfony/twig-bridge": "^3.3|^4.0",
    "symfony/yaml": "^3.3|^4.0",
    "twig/twig": "^1.12"
  },
  "suggest": {
    "egeloen/form-extra-bundle": "Allows to load CKEditor asynchronously",
    "sensio/distribution-bundle": "Allows to install CKEditor via a script handler for symfony < 4",
    "symfony/asset": "Allows to rewrite/version assets",
    "symfony/templating": "Allows to use PHP templates",
    "symfony/twig-bridge": "Allows to use Twig templates",
    "twig/twig": "Allows to use Twig templates"
  },
  "autoload": {
    "psr-4": {
      "Ivory\\CKEditorBundle\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.0-dev"
    }
  }
}
